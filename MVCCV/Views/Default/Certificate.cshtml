@using MVCCV.Models.Entity
@model List<Certificate>
<div class="resume-section-content">
    <h2 class="mb-5">Sertifikalarim</h2>
    <ul class="fa-ul mb-0">
        @foreach (var x in Model)
        {
            <li>
                <span class="fa-li"><i class="fas fa-trophy text-warning"></i></span>
                @x.Definition
            </li>
        }
        </ul>
</div>
