@model MVCCV.Models.Entity.SocialMedia
@{
    ViewBag.Title = "SkillGet";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h2>Yetenek Düzenle</h2>

<div class="resume-section-content">
    <form method="post">
        @Html.Label("Sosyal Medya Adi")
        @Html.TextBoxFor(x => x.SocialName, new { @class = "form-control" })
        <br />
        @Html.Label("Link")
        @Html.TextBoxFor(x => x.Link, new { @class = "form-control" })
        <br />
        @Html.Label("Ikon")
        @Html.TextBoxFor(x => x.Icon, new { @class = "form-control" })
        <br />
        <br />
        <button class="btn btn-info">Güncelle</button>
    </form>
</div>


